{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/check-all-notifications",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/reset-daily-flags",
      "schedule": "1 0 * * *"
    }
  ]
}